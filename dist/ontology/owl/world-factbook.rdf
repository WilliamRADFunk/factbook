  //
  ///////////////////////////////////////////////////////////////////////////////////////
  -->

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasNationalFlag -->

  <ObjectProperty rdf:about="&wfact;hasNationalFlag">
      <rdfs:domain rdf:resource="&wfact;Country"/>
      <rdfs:range rdf:resource="&wfact;NationalFlag"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasBorderMap -->

  <ObjectProperty rdf:about="&wfact;hasBorderMap">
      <rdfs:domain rdf:resource="&wfact;Country"/>
      <rdfs:range rdf:resource="&wfact;BorderMap"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasRegionMap -->

  <ObjectProperty rdf:about="&wfact;hasRegionMap">
      <rdfs:domain rdf:resource="&wfact;Country"/>
      <rdfs:range rdf:resource="&wfact;RegionMap"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasSupplementalImage -->

  <ObjectProperty rdf:about="&wfact;hasSupplementalImage">
      <rdfs:domain rdf:resource="&wfact;Country"/>
      <rdfs:range rdf:resource="&wfact;Image"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasDomainArea -->

  <ObjectProperty rdf:about="&wfact;hasDomainArea">
      <rdfs:domain rdf:resource="&wfact;Country"/>
      <rdfs:range rdf:resource="&wfact;DomainArea"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasCoast -->

  <ObjectProperty rdf:about="&wfact;hasCoast">
      <rdfs:domain rdf:resource="&wfact;Country"/>
      <rdfs:range rdf:resource="&wfact;Coast"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasClimate -->

  <ObjectProperty rdf:about="&wfact;hasClimate">
      <rdfs:domain rdf:resource="&wfact;Country"/>
      <rdfs:range rdf:resource="&wfact;Climate"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasClimateZone -->

  <ObjectProperty rdf:about="&wfact;hasClimateZone">
      <rdfs:domain rdf:resource="&wfact;Climate"/>
      <rdfs:range rdf:resource="&wfact;ClimateZone"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasBorderCountryPair -->

  <ObjectProperty rdf:about="&wfact;hasBorderCountryPair">
      <rdfs:domain rdf:resource="&wfact;Country"/>
      <rdfs:range rdf:resource="&wfact;BorderCountryPair"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasCountry -->

  <ObjectProperty rdf:about="&wfact;hasCountry">
      <rdfs:domain rdf:resource="&wfact;BorderCountryPair"/>
      <rdfs:range rdf:resource="&wfact;Country"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasBorder -->

  <ObjectProperty rdf:about="&wfact;hasBorder">
      <rdfs:domain rdf:resource="&wfact;Country"/>
      <rdfs:range rdf:resource="&wfact;Border"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasMaritimeClaim -->

  <ObjectProperty rdf:about="&wfact;hasMaritimeClaim">
      <rdfs:domain rdf:resource="&wfact;Country"/>
      <rdfs:range rdf:resource="&wfact;MaritimeClaim"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasNaturalResource -->

  <ObjectProperty rdf:about="&wfact;hasNaturalResource">
      <rdfs:domain rdf:resource="&wfact;Country"/>
      <rdfs:range rdf:resource="&wfact;NaturalResource"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasElevation -->

  <ObjectProperty rdf:about="&wfact;hasElevation">
      <rdfs:domain rdf:resource="&wfact;Country"/>
      <rdfs:range rdf:resource="&wfact;Elevation"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasTerrain -->

  <ObjectProperty rdf:about="&wfact;hasTerrain">
      <rdfs:domain rdf:resource="&wfact;Country"/>
      <rdfs:range rdf:resource="&wfact;Terrain"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasLandUse -->

  <ObjectProperty rdf:about="&wfact;hasLandUse">
      <rdfs:domain rdf:resource="&wfact;Country"/>
      <rdfs:range rdf:resource="&wfact;LandUse"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasAgriculturalLand -->

  <ObjectProperty rdf:about="&wfact;hasAgriculturalLand">
      <rdfs:domain rdf:resource="&wfact;LandUse"/>
      <rdfs:range rdf:resource="&wfact;AgriculturalLand"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasArableLand -->

  <ObjectProperty rdf:about="&wfact;hasArableLand">
      <rdfs:domain rdf:resource="&wfact;LandUse"/>
      <rdfs:range rdf:resource="&wfact;ArableLand"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasForestLand -->

  <ObjectProperty rdf:about="&wfact;hasForestLand">
      <rdfs:domain rdf:resource="&wfact;LandUse"/>
      <rdfs:range rdf:resource="&wfact;ForestLand"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasOtherLand -->

  <ObjectProperty rdf:about="&wfact;hasOtherLand">
      <rdfs:domain rdf:resource="&wfact;LandUse"/>
      <rdfs:range rdf:resource="&wfact;OtherLand"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasPermanentCropsLand -->

  <ObjectProperty rdf:about="&wfact;hasPermanentCropsLand">
      <rdfs:domain rdf:resource="&wfact;LandUse"/>
      <rdfs:range rdf:resource="&wfact;PermanentCropsLand"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasPermanentPastureLand -->

  <ObjectProperty rdf:about="&wfact;hasPermanentPastureLand">
      <rdfs:domain rdf:resource="&wfact;LandUse"/>
      <rdfs:range rdf:resource="&wfact;PermanentPastureLand"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasArtificiallyIrrigatedLand -->

  <ObjectProperty rdf:about="&wfact;hasArtificiallyIrrigatedLand">
      <rdfs:domain rdf:resource="&wfact;Country"/>
      <rdfs:range rdf:resource="&wfact;ArtificiallyIrrigatedLand"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasNaturalHazard -->

  <ObjectProperty rdf:about="&wfact;hasNaturalHazard">
      <rdfs:domain rdf:resource="&wfact;Country"/>
      <rdfs:range rdf:resource="&wfact;NaturalHazard"/>
  </ObjectProperty>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#hasGeographicNote -->

  <ObjectProperty rdf:about="&wfact;hasGeographicNote">
