<!DOCTYPE rdf:RDF [
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY wfact "http://williamrobertfunk.com/ontologies/world-factbook#" >
    <!ENTITY pos "http://www.w3.org/2003/01/geo/wgs84_pos#">
    <!ENTITY dafu "http://www.daedafusion.com/">
    <!ENTITY dbp "http://dbpedia.org/ontology/">
    <!ENTITY blade "http://modusoperandi.com/ont/blade#">
]>

<rdf:RDF xmlns="http://www.w3.org/2002/07/owl#"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  xmlns:wfact="http://williamrobertfunk.com/ontologies/world-factbook#"
  xmlns:pos="http://www.w3.org/2003/01/geo/wgs84_pos#"
  xmlns:dafu="http://www.daedafusion.com/"
  xmlns:dbp="http://dbpedia.org/ontology"
  xmlns:blade="http://modusoperandi.com/ont/blade#">

  <Ontology rdf:about="http://williamrobertfunk.com/ontologies/world-factbook">
    <versionInfo rdf:datatype="&xsd;decimal">1.5</versionInfo>
    <dc:rights>Copyright 2019 William R.A.D. Funk. All rights reserved.</dc:rights>
    <dc:creator>William R.A.D. Funk</dc:creator>
    <dc:contributor>William R.A.D. Funk</dc:contributor>
    <rdfs:comment>
      Identifies application specific classes and annotations that are relevant
      only to my associated applications.
    </rdfs:comment>
    <rdfs:label>Funk Ontology</rdfs:label>
  </Ontology>

  <!--
  ///////////////////////////////////////////////////////////////////////////////////////
  //
  // Classes
  //
  ///////////////////////////////////////////////////////////////////////////////////////
  -->

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#Country -->

  <Class rdf:about="&wfact;Country">
    <rdfs:label>Country</rdfs:label>
    <rdfs:comment>a nation with its own government, occupying a particular territory.</rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
    <equivalentClass rdf:resource="&dbp;Country" />
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#NationalFlag -->

  <Class rdf:about="&wfact;NationalFlag">
    <rdfs:label>NationalFlag</rdfs:label>
    <rdfs:comment>
      a piece of cloth flown by the government of a country,
      and designed with specific meanings for its colours and symbols.
    </rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#Image -->

  <Class rdf:about="&wfact;Image">
    <rdfs:label>Image</rdfs:label>
    <rdfs:comment>a digitally stored image</rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
    <rdfs:subClassOf rdf:resource="&dafu;Asset"/>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#BorderMap -->

  <Class rdf:about="&wfact;BorderMap">
    <rdfs:label>Border Map Image</rdfs:label>
    <rdfs:comment>an image showing a country's full border and direct neighbors.</rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
    <rdfs:subClassOf rdf:resource="&wfact;Image"/>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#RegionMap -->

  <Class rdf:about="&wfact;RegionMap">
    <rdfs:label>Region Map Image</rdfs:label>
    <rdfs:comment>an image showing a country's position within a region.</rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
    <rdfs:subClassOf rdf:resource="&wfact;Image"/>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#DomainArea -->

  <Class rdf:about="&wfact;DomainArea">
    <rdfs:label>Domain Area</rdfs:label>
    <rdfs:comment>Area within a country's borders (ie. Land, Water, Total).</rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#Coast -->

  <Class rdf:about="&wfact;Coast">
    <rdfs:label>Coast</rdfs:label>
    <rdfs:comment>Geographic section where the land borders the sea.</rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#Climate -->

  <Class rdf:about="&wfact;Climate">
    <rdfs:label>Climate</rdfs:label>
    <rdfs:comment>The weather conditions prevailing in an area in general or over a long period.</rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#ClimateZone -->

  <Class rdf:about="&wfact;ClimateZone">
    <rdfs:label>Climate Zone</rdfs:label>
    <rdfs:comment>
      Divisions of the Earth's climates into general climate zones according to average temperatures and average rainfall.
      The three major climate zones on the Earth are the polar, temperate, and tropical zones.
    </rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#BorderCountryPair -->

  <Class rdf:about="&wfact;BorderCountryPair">
    <rdfs:label>Border Country Pair</rdfs:label>
    <rdfs:comment>Two countries that share a land border.</rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#Border -->

  <Class rdf:about="&wfact;Border">
    <rdfs:label>National Border</rdfs:label>
    <rdfs:comment>Total border belonging to a soverign nation.</rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#MaritimeClaim -->

  <Class rdf:about="&wfact;MaritimeClaim">
    <rdfs:label>Maritime Claim</rdfs:label>
    <rdfs:comment>
      The adjacent belt of sea that extends beyond a coastal state's land territory and internal waters.
      Includes airspace over that belt of sea.
    </rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#NaturalResource -->

  <Class rdf:about="&wfact;NaturalResource">
    <rdfs:label>Natural Resource</rdfs:label>
    <rdfs:comment>
      Material or substance such as minerals, forests, water,and fertile land
      that occur in nature and can be used for economic gain.
    </rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#Elevation -->

  <Class rdf:about="&wfact;Elevation">
    <rdfs:label>Elevation</rdfs:label>
    <rdfs:comment>
      The vertical distance above the reference geoid,
      a mathematical model of the Earth's sea level as an equipotential gravitational surface.
    </rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#Terrain -->

  <Class rdf:about="&wfact;Terrain">
    <rdfs:label>Terrain</rdfs:label>
    <rdfs:comment>A stretch of land, especially with regard to its physical features.</rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#LandUse -->

  <Class rdf:about="&wfact;LandUse">
    <rdfs:label>Land Use</rdfs:label>
    <rdfs:comment>A break down of uses for the land (ie. agricultural, forest, pasture, etc.).</rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#AgriculturalLand -->

  <Class rdf:about="&wfact;AgriculturalLand">
    <rdfs:label>Agricultural Land</rdfs:label>
    <rdfs:comment>Percentage of land used for agricultural purposes.</rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#ArableLand -->

  <Class rdf:about="&wfact;ArableLand">
    <rdfs:label>Arable Land</rdfs:label>
    <rdfs:comment>Percentage of land set aside to be arable.</rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#PermanentCropsLand -->

  <Class rdf:about="&wfact;PermanentCropsLand">
    <rdfs:label>Permanent Crops Land</rdfs:label>
    <rdfs:comment>Percentage of land set aside permanently for use in growing crops.</rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#PermanentPastureLand -->

  <Class rdf:about="&wfact;PermanentPastureLand">
    <rdfs:label>Permanent Pasture Land</rdfs:label>
    <rdfs:comment>Percentage of land set aside permanently for use as pasture.</rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#OtherLand -->

  <Class rdf:about="&wfact;OtherLand">
    <rdfs:label>Other Land</rdfs:label>
    <rdfs:comment>Percentage of land used for purposes other that agricultural, grazing, or forest.</rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
    <rdfs:subClassOf rdf:resource="&wfact;LandUse"/>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#ArtificiallyIrrigatedLand -->

  <Class rdf:about="&wfact;ArtificiallyIrrigatedLand">
    <rdfs:label>Artificially Irrigated Land</rdfs:label>
    <rdfs:comment>
      Percentage of land artificially irrigated.
    </rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#NaturalHazard -->

  <Class rdf:about="&wfact;NaturalHazard">
    <rdfs:label>Natural Hazard</rdfs:label>
    <rdfs:comment>
      A natural phenomenon that might have a negative effect on humans or the environment.
    </rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#GeographicNote -->

  <Class rdf:about="&wfact;GeographicNote">
    <rdfs:label>Geographic Note</rdfs:label>
    <rdfs:comment>
      An additional description on a place's geography.
      The significance of it's physical location in the larger world.
    </rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
  </Class>

  <!-- http://williamrobertfunk.com/ontologies/world-factbook#Location -->

  <Class rdf:about="&wfact;Location">
    <rdfs:label>Location</rdfs:label>
    <rdfs:comment>
      A location defined by latitude and longitude according to the wgs84 definition.
    </rdfs:comment>
    <blade:navClass rdf:datatype="&xsd;boolean">true</blade:navClass>
  </Class>

  <!--
  ///////////////////////////////////////////////////////////////////////////////////////
