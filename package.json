{
  "name": "factbook",
  "version": "0.1.0",
  "description": "Serves as an independent data scraping module, complete with ontology and full scraping ability for the CIA World Factbook site",
  "main": "index.js",
  "scripts": {
    "clean:dist": "rimraf dist/**/*.json",
    "clean:logs": "rimraf logs/*log*",
    "copy:ontology": "npm-run-all copy:jsonld copy:owl",
    "copy:jsonld": "copyfiles -u 1 ontology/*.json dist/ontology/jsonld/",
    "copy:owl": "copyfiles -u 1 ontology/*.rdf dist/ontology/owl/",
    "scrape": "ts-node index.ts",
    "start": "run-s clean:logs clean:dist scrape copy:ontology",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WilliamRADFunk/factbook.git"
  },
  "keywords": [
    "data",
    "scraping",
    "big",
    "data",
    "data",
    "scraping",
    "ontology",
    "json",
    "json-ld",
    "CIA",
    "factbook",
    "world",
    "factbook",
    "world-factbook",
    "factbook.json",
    "countries",
    "data",
    "big",
    "data"
  ],
  "author": "William RAD Funk",
  "license": "UNLICENSED",
  "dependencies": {
    "@types/cheerio": "^0.22.11",
    "cheerio": "^1.0.0-rc.2",
    "copyfiles": "^2.1.0",
    "html-to-text": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "request": "^2.34.0",
    "request-promise": "^4.2.4",
    "rimraf": "^2.6.3",
    "simple-node-logger": "^18.12.22",
    "uuid-by-string": "^2.1.0"
  },
  "devDependencies": {
    "formatter-json": "^0.1.1",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3333"
  }
}
